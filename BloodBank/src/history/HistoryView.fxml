<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="300.0" minWidth="500.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="history.HistoryViewController">
   <children>
      <Label alignment="CENTER" prefHeight="60.0" prefWidth="500.0" style="-fx-background-color: black; -fx-border-color: white; -fx-border-radius: 3; -fx-border-width: 3; -fx-background-radius: 6;" text="History View" textFill="WHITE">
         <font>
            <Font name="Harrington" size="35.0" />
         </font>
      </Label>
      <Button layoutX="35.0" layoutY="93.0" mnemonicParsing="false" onAction="#doinfo" prefHeight="43.0" prefWidth="171.0" style="-fx-background-color: white; -fx-border-color: black; -fx-border-radius: 5; -fx-border-width: 2.5;" text="Donor Info">
         <font>
            <Font name="Georgia Bold" size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="202.0" layoutY="227.0" mnemonicParsing="false" onAction="#dohome" style="-fx-background-color: white; -fx-border-color: black; -fx-border-radius: 5; -fx-border-width: 2.5;" text="Home">
         <font>
            <Font name="Georgia Bold" size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="164.0" layoutY="157.0" mnemonicParsing="false" onAction="#doissue" style="-fx-background-color: white; -fx-border-color: black; -fx-border-radius: 5; -fx-border-width: 2.5;" text="Blood Issued ">
         <font>
            <Font name="Georgia Bold" size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="283.0" layoutY="93.0" mnemonicParsing="false" onAction="#docollection" style="-fx-background-color: white; -fx-border-color: black; -fx-border-radius: 5; -fx-border-width: 2.5;" text="Blood Collection">
         <font>
            <Font name="Georgia Bold" size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <ImageView fitHeight="43.0" fitWidth="59.0" layoutX="427.0" layoutY="9.0">
         <image>
            <Image url="@../../pics/history.png" />
         </image>
      </ImageView>
      <ImageView fx:id="imghist" fitHeight="43.0" fitWidth="59.0" layoutX="14.0" layoutY="9.0">
         <image>
            <Image url="@../../pics/clock.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
